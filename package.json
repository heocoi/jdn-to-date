{
  "name": "jdn-to-date",
  "version": "0.1.1",
  "description": "A npm package for converting Julian Day Number (JDN) to Date object.",
  "license": "MIT",
  "repository": "heocoi/jdn-to-date",
  "author": {
    "name": "heocoi",
    "email": "anhphong.pham@gmail.com",
    "url": "https://github.com/heocoi"
  },
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "julian",
    "julian day number",
    "julian calendar",
    "date"
  ],
  "dependencies": {
    "timezone-mock": "^1.3.6"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "xo": "^0.54.2"
  },
  "xo": {
    "space": true,
    "rules": {
      "unicorn/empty-brace-spaces": "off",
      "unicorn/numeric-separators-style": "off",
      "no-mixed-operators": "off",
      "unicorn/prevent-abbreviations": "off"
    }
  }
}
